<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.smartcompany.job.modules.core.mapper.TmgNotification.TmgNotificationMapper">

    <select id="selectEmoloyMail" resultType="jp.smartcompany.admin.usermanager.dto.UserManagerListDTO">
        SELECT
            TMA_ID, TMA_EMP_ID,TMA_EMP_NAME,TMA_EMAIL
        FROM
            T_EMPLOY_MAIL
        WHERE
            TMA_EMP_ID = #{keyword}
        ORDER BY TMA_EMP_ID
    </select>

</mapper>